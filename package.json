{
  "name": "@oliver-schoendorn/babel-plugin-transform-define",
  "version": "2.0.0",
  "description": "This babel plugin will take a set of config values (or load them from a file path) and replace all occurrences in the parsed scripts.",
  "keywords": [
    "babel",
    "plugin",
    "define"
  ],
  "repository": "github:oliver-schoendorn/babel-plugin-transform-define",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "clear": "rimraf ./dist/**/*",
    "build": "npm run clear && babel ./src -d ./dist --extensions \".ts\"",
    "build:dev": "NODE_ENV=test npm run build",
    "test": "NODE_ENV=test npm run build && jest --coverage"
  },
  "author": "Oliver Sch√∂ndorn <info@oswebstyle.de>",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testRegex": "(/test/.*\\.(test|spec))\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "./dist/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/traverse": "^7.7.4",
    "@types/babel-core": "^6.25.6",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel-types": "^7.0.7",
    "@types/jest": "^24.0.23",
    "@types/node": "^11.15.3",
    "core-js": "^2.6.10",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "rimraf": "^2.7.1",
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  }
}
