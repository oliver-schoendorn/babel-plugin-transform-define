{
  "presets": [
    "@babel/typescript",
    ["@babel/env", {
      "useBuiltIns": "usage",
      "targets": { "node": "current" }
    }]
  ],
  "plugins": [
    [ "@babel/plugin-proposal-class-properties", { "loose": false } ]
  ],
  "compact": true,
  "comments": false,
  "env": {
    "test": {
      "sourceMaps": "inline",
      "compact": false,
      "comments": true
    }
  }
}
